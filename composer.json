{
    "name": "genealogiawebsite/helpers",
    "description": "Helper classes dependency for Laravel Enso",
    "keywords": [
        "genealogiawebsite",
        "helpers"
    ],
    "homepage": "https://github.com/genealogiawebsite/helpers",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://genealogiawebsite.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "laravel/framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Helpers\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "EnsoException": "LaravelEnso\\Helpers\\Exceptions\\EnsoException"
            }
        }
    }
}
